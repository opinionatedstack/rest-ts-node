    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-jwt-authz": "^2.4.1",
    "helmet": "^3.23.3",
    "http-errors": "^1.8.0",
    "jwks-rsa": "^1.12.0",
