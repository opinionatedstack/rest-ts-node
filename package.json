  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc",
    "dev": "DOTENV_PATH=./environments/.env.local ts-node-dev --respawn --transpileOnly ./src/app.ts",
